{
  "name": "speakweb",
  "displayName": "Speakweb",
  "description": "VS Code extension for displaying selected text",
  "version": "0.0.1",
  "publisher": "YourPublisherName",
  "engines": {
      "vscode": "^1.0.0"
  },
  "categories": [
      "Other"
  ],
  "activationEvents": [
      "onCommand:extension.showSelectedText"
  ],
  "main": "./out/extension.js",
  "contributes": {
      "commands": [
          {
              "command": "extension.showSelectedText",
              "title": "Show Selected Text"
          }
      ]
  },
  "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./",
      "pretest": "npm run compile",
      "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
      "@types/glob": "^7.1.1",
      "@types/mocha": "^5.2.6",
      "@types/node": "^10.12.18",
      "@typescript-eslint/eslint-plugin": "^2.33.0",
      "@typescript-eslint/parser": "^2.33.0",
      "eslint": "^6.8.0",
      "glob": "^7.1.6",
      "mocha": "^6.2.2",
      "typescript": "^3.7.2",
      "vscode": "^1.0.0",
      "vscode-test": "^1.1.2"
  }
}
